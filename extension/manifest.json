{
    "name": "missmarple",
    "version": "0.1",
    "minimum_chrome_version": "25.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "devtools_page": "devtools.html",
    "content_scripts": [{
        "matches": ["https://*/*", "http://*/*", "file://*/*"],
        "js": ["content-script.js"]
    }],
    "background": {
        "scripts": ["background-script.js"]
    }
}
